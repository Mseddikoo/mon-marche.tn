{% extends '::layout/layoutAdmin.html.twig' %}

{% block body -%}

    <h1>Adresses de l'utilisateur </h1>
    {% for adresse in utilisateur.adresses %}
        <div class="span12">
    <div class="span4">
        <h4>Adresse {{ loop.index }}:</h4>
        {{ adresse.adresse }}, {{ adresse.cp }} {{ adresse.ville }} -{{ adresse.paye }}
        <br>{{ adresse.prenom }} {{ adresse.nom }}
        <br/>
        <br/>
    </div>
        </div>
    {% endfor %}
{% endblock %}
